<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="bookings">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="booking" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="booking_id" type="xs:int"/>
                            <xs:element name="customer_id" type="xs:int"/>
                            <xs:element name="room_id" type="xs:int"/>
                            <xs:element name="booking_date" type="xs:date"/>
                            <xs:element name="check_in_date" type="xs:date"/>
                            <xs:element name="check_out_date" type="xs:date"/>
                            <xs:element name="total_price" type="xs:decimal"/>
                            <xs:element name="payment_status" type="xs:boolean"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>